#!/usr/bin/env lims-perl

use strict;
use warnings;

use Carp::Heavy; # so FindBin doesn't break
use FindBin;

use lib "$FindBin::Bin/../../perl5/lib/perl5";
use lib "$FindBin::Bin/../../refimp/lib";

BEGIN {
    if ( ! exists $ENV{REFIMP_CONFIG_FILE} ) {
        $ENV{REFIMP_CONFIG_FILE} = "$FindBin::Bin/../../refimp-config/prod.yml";
    }
};

use above "RefImp";
use RefImp::Command;
RefImp::Command->execute_with_shell_params_and_exit();
